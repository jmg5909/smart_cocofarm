<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Smart_cocofarm" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#CAEEBE</item>
        <item name="colorPrimaryVariant">#CAEEBE</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:textViewStyle">@font/nanumgothic_regular</item>
        <item name="android:buttonStyle">@font/nanumgothic_regular</item>
        <item name="android:editTextStyle">@font/nanumgothic_regular</item>
        <item name="android:radioButtonStyle">@font/nanumgothic_regular</item>
        <item name="android:checkboxStyle">@font/nanumgothic_regular</item>
    </style>
</resources>